<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/cropListFragment">

    <fragment
        android:id="@+id/cropListFragment"
        android:name="com.example.harvest.crop.CropListFragment"
        android:label="fragment_crop_list"
        tools:layout="@layout/fragment_crop_list" >
        <action
            android:id="@+id/action_cropListFragment_to_add_crop_graph"
            app:destination="@id/add_crop_graph" />
    </fragment>
    <navigation android:id="@+id/add_crop_graph"
        app:startDestination="@id/cropAddFragment">
        <fragment
            android:id="@+id/cropAddFragment"
            android:name="com.example.harvest.crop.CropAddFragment"
            android:label="fragment_crop_add"
            tools:layout="@layout/fragment_crop_add">
            <action
                android:id="@+id/action_cropAddFragment_to_plantListFragment"
                app:destination="@id/select_plant_graph" />
        </fragment>
        <action
            android:id="@+id/action_global_cropListFragment"
            app:destination="@id/cropListFragment" />
        <navigation android:id="@+id/select_plant_graph"
            app:startDestination="@id/plantListFragment">
            <fragment
                android:id="@+id/plantListFragment"
                android:name="com.example.harvest.plant.PlantListFragment"
                android:label="fragment_plant_list"
                tools:layout="@layout/fragment_plant_list">
                <action
                    android:id="@+id/action_plantListFragment_to_plantAddFragment"
                    app:destination="@id/plantAddFragment" />
            </fragment>
            <fragment
                android:id="@+id/plantAddFragment"
                android:name="com.example.harvest.plant.PlantAddFragment"
                android:label="fragment_plant_add"
                tools:layout="@layout/fragment_plant_add">
                <action
                    android:id="@+id/action_plantAddFragment_to_plantListFragment"
                    app:destination="@id/plantListFragment" />
            </fragment>
            <action
                android:id="@+id/action_global_cropAddFragment"
                app:destination="@id/cropAddFragment" />
        </navigation>
    </navigation>
</navigation>